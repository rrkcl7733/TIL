   "source": [
    "m = int(input())\n",
    "n = int(input())\n",
    "tot = []\n",
    "for i in range(m, n + 1):\n",
    "    if (i ** .5).is_integer():\n",
    "        tot += [i]\n",
    "if tot:\n",
    "    print(sum(tot), tot[0], sep='\\n')\n",
    "else:\n",
    "    print(-1)"
   ]
